using System;
using System.Collections.Generic;
using System.Text;

namespace FM2E.Model.Maintain
{
    /// <summary>
    /// 故障类别查询实体类
    /// </summary>
    public class MalfunctionClassifySearchInfo
    {
        #region model
        private string system;
        private long subSystem;
        private string malfunctionObject;
        private int rank;
        #endregion
        /// <summary>
        /// 系统ID
        /// </summary>
        public string System
        {
            get { return system; }
            set { system = value; }
        }
        /// <summary>
        /// 子系统ID
        /// </summary>
        public long SubSystem
        {
            get { return subSystem; }
            set { subSystem = value; }
        }

        /// <summary>
        /// 故障对象
        /// </summary>
        public string MalfunctionObject
        {
            get { return malfunctionObject; }
            set { malfunctionObject = value; }
        }

        /// <summary>
        /// 响应类型
        /// </summary>
        public int Rank
        {
            get { return rank; }
            set { rank = value; }
        }
    }
}
