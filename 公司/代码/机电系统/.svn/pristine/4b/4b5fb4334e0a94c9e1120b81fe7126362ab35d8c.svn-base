using System;
using System.Collections.Generic;
using System.Text;

namespace FM2E.Model.SpecialProject
{
    /// <summary>
    /// 专项工程支付记录（月支付）实体类
    /// </summary>
    public class SpecialProjectPayRecordInfo
    {
        public SpecialProjectPayRecordInfo()
		{}
		#region Model
		private long _projectid;
		private string _remark;
		private int _year;
		private int _month;
		private DateTime _paytime;
		private decimal _progress;
		private decimal _amount;
		private string _method;
		private decimal _paid;
		private string _payee;
		/// <summary>
		/// 专项工程ID 
		/// </summary>
        public long ProjectID
		{
			set{ _projectid=value;}
			get{return _projectid;}
		}
		/// <summary>
		/// 备注
		/// </summary>
		public string Remark
		{
			set{ _remark=value;}
			get{return _remark;}
		}
		/// <summary>
		/// 年
		/// </summary>
		public int Year
		{
			set{ _year=value;}
			get{return _year;}
		}
		/// <summary>
		/// 月
		/// </summary>
		public int Month
		{
			set{ _month=value;}
			get{return _month;}
		}
		/// <summary>
		/// 时间
		/// </summary>
		public DateTime PayTime
		{
			set{ _paytime=value;}
			get{return _paytime;}
		}
		/// <summary>
		/// 当前进度百分比
		/// </summary>
		public decimal Progress
		{
			set{ _progress=value;}
			get{return _progress;}
		}
		/// <summary>
		/// 金额
		/// </summary>
		public decimal Amount
		{
			set{ _amount=value;}
			get{return _amount;}
		}
		/// <summary>
		/// 支付方式
		/// </summary>
		public string Method
		{
			set{ _method=value;}
			get{return _method;}
		}
		/// <summary>
		/// 支付金额
		/// </summary>
		public decimal Paid
		{
			set{ _paid=value;}
			get{return _paid;}
		}
		/// <summary>
		/// 支付人
		/// </summary>
		public string Payee
		{
			set{ _payee=value;}
			get{return _payee;}
		}
		#endregion Model
    }
}
