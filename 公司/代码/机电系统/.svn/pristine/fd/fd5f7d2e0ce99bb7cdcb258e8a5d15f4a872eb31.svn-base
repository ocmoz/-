using System;
using System.Collections.Generic;
using System.Text;

namespace FM2E.Model.SpecialProject
{
    /// <summary>
    /// 设备进场记录
    /// </summary>
    public class SpecialProjectDeviceInRecord
    {
        public SpecialProjectDeviceInRecord()
		{}
		#region Model
        private long _recordid;
        private long _itemid;
        private long _projectid;
		private decimal _count;
		private DateTime _time;
		/// <summary>
		/// 记录数据库ID
		/// </summary>
        public long RecordID
		{
			set{ _recordid=value;}
			get{return _recordid;}
		}
		/// <summary>
		/// 进场设备项ID
		/// </summary>
		public long ItemID
		{
			set{ _itemid=value;}
			get{return _itemid;}
		}
		/// <summary>
		/// 专项工程ID
		/// </summary>
        public long ProjectID
		{
			set{ _projectid=value;}
			get{return _projectid;}
		}
		/// <summary>
		/// 本次进场数量
		/// </summary>
		public decimal Count
		{
			set{ _count=value;}
			get{return _count;}
		}
		/// <summary>
		/// 进场时间
		/// </summary>
		public DateTime Time
		{
			set{ _time=value;}
			get{return _time;}
		}
		#endregion Model
    }
}
