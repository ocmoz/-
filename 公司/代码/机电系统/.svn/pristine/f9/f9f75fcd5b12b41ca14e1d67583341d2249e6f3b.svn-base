using System;
using System.Collections.Generic;
using System.Text;

namespace FM2E.Model.Maintain
{
    public class MaintainedEquipmentRateInfo
    {
        private int _equipmentno;   //序号
        private string _equipmentname;  //设备名称
        private int _allequipmentcount;  //已安装设备数量
        private int _wait4repairedequipmentcount;  //故障仍未修复数量
        private int _maintainedequipmentcount;  //故障台数
        private int _repairedequipmentcount;  //故障修复台数
        private int _unrepairedequipmentcount;  //故障末修复数
        private int _normalequipmentcount;  //设备完好数量
        private decimal _normalequipmentrate;  //设备完好率
        private string _remark;  //备注

        //序号
        public int EquipmentNO
        {
            get { return _equipmentno; }
            set { _equipmentno = value; }
        }

        //设备名称
        public string EquipmentName
        {
            get { return _equipmentname; }
            set { _equipmentname = value; }
        }

        //已安装设备数量
        public int AllEquipmentCount
        {
            get { return _allequipmentcount; }
            set { _allequipmentcount = value; }
        }

        //故障仍未修复数量
        public int Wait4RepairedEquipmentCount
        {
            get { return _wait4repairedequipmentcount; }
            set { _wait4repairedequipmentcount = value; }
        }

        //故障台数
        public int MaintainedEquipmentCount
        {
            get { return _maintainedequipmentcount; }
            set { _maintainedequipmentcount = value; }
        }

        //故障修复台数
        public int RepairedEquipmentCount
        {
            get { return _repairedequipmentcount; }
            set { _repairedequipmentcount = value; }
        }

        //故障末修复数
        public int UnRepairedEquipmentCount
        {
            get { return _unrepairedequipmentcount; }
            set { _unrepairedequipmentcount = value; }
        }

        //设备完好数量
        public int NormalEquipmentCount
        {
            get { return _normalequipmentcount; }
            set { _normalequipmentcount = value; }
        }

        //设备完好率
        public decimal NormalEquipmentRate
        {
            get { return _normalequipmentrate; }
            set { _normalequipmentrate = value; }
        }

        //备注
        public string Remark
        {
            get { return _remark; }
            set { _remark = value; }
        }
    }
}
