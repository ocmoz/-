using System;
using System.Collections.Generic;
using System.Text;

namespace FM2E.Model.Equipment
{
    /// <summary>
    /// 不经申请的采购验收回来后的条码信息
    /// </summary>
    public class CheckAcceptanceDetailBarcodeInfo
    {
        public CheckAcceptanceDetailBarcodeInfo()
		{}
		#region Model
        private long _id;
        private string _companyid = "";
		private long _formid;
		private short _itemid;
        private string _barcode = "";
        private string _productname = "";
        private string _model = "";
		/// <summary>
		/// 数据库自增ID
		/// </summary>
        public long ID
		{
			set{ _id=value;}
			get{return _id;}
		}

        /// <summary>
        /// 公司id
        /// </summary>
        public string CompanyID
        {
            set { _companyid = value; }
            get { return _companyid; }
        }

		/// <summary>
		/// 表单ID
		/// </summary>
        public long FormID
		{
			set{ _formid=value;}
			get{return _formid;}
		}
		/// <summary>
		/// 详细项序号
		/// </summary>
		public short ItemID
		{
			set{ _itemid=value;}
			get{return _itemid;}
		}
		/// <summary>
		/// 条码
		/// </summary>
		public string Barcode
		{
			set{ _barcode=value;}
			get{return _barcode;}
		}
		/// <summary>
		/// 产品名称
		/// </summary>
		public string ProductName
		{
			set{ _productname=value;}
			get{return _productname;}
		}
		/// <summary>
		/// 产品型号
		/// </summary>
		public string Model
		{
			set{ _model=value;}
			get{return _model;}
		}
		#endregion Model
    }
}
