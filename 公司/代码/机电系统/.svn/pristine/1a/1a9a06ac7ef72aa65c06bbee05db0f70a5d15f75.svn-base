using System;
using System.Collections.Generic;
using System.Text;

namespace FM2E.Model.Equipment
{
    /// <summary>
    /// 设备库存信息，包括公司ID、仓库ID、设备名称、规格型号、库存数量
    /// </summary>
    public class EquipmentStorageInfo
    {
        private string _companyid;
        private string _warehouseid;
        private string _warehousename;
        private string _equipmentname;
        private string _equipmentmodel;
        private decimal _storage;
        private string _unit;
        private int _warming;

        /// <summary>
        /// 公司ID
        /// </summary>
        public string CompanyID
        {
            get { return _companyid; }
            set { _companyid = value; }
        }

        /// <summary>
        /// 仓库ID
        /// </summary>
        public string WareHouseID
        {
            get { return _warehouseid; }
            set { _warehouseid = value; }
        }

        /// <summary>
        /// 仓库名称
        /// </summary>
        public string WareHouseName
        {
            get { return _warehousename; }
            set { _warehousename = value; }
        }
        /// <summary>
        /// 设备名称
        /// </summary>
        public string EquipmentName
        {
            get { return _equipmentname; }
            set { _equipmentname = value; }
        }
        /// <summary>
        /// 规格型号
        /// </summary>
        public string EquipmentModel
        {
            get { return _equipmentmodel; }
            set { _equipmentmodel = value; }
        }
        /// <summary>
        /// 库存量
        /// </summary>
        public decimal Storage
        {
            get { return _storage; }
            set { _storage = value; }
        }

        /// <summary>
        /// 单位
        /// </summary>
        public string Unit
        {
            get { return _unit; }
            set { _unit = value; }
        }
        /// <summary>
        /// 单位
        /// </summary>
        public int Warming
        {
            get { return _warming; }
            set { _warming = value; }
        }
    }
}
