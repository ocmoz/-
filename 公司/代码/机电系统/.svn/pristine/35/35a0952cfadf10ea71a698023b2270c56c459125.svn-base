using System;
using System.Collections.Generic;
using System.Text;

namespace FM2E.Model.Equipment
{
    public class WareHouseCheckSearchInfo
    {
        private string formNO;
        private string wareHouseID;
        private string companyID;
        private string checker;
        private WareHouseFormStatus status;
        private DateTime checkDateFrom;
        private DateTime checkDateTo;

        /// <summary>
        /// 表单编号
        /// </summary>
        public string FormNO
        {
            get { return formNO; }
            set { formNO = value; }
        }
        /// <summary>
        /// 检查的仓库编号
        /// </summary>
        public string WareHouseID
        {
            get { return wareHouseID; }
            set { wareHouseID = value; }
        }
        /// <summary>
        /// 公司ID
        /// </summary>
        public string CompanyID
        {
            get { return companyID; }
            set { companyID = value; }
        }
        /// <summary>
        /// 检查人用户名
        /// </summary>
        public string Checker
        {
            get { return checker; }
            set { checker = value; }
        }
        /// <summary>
        /// 检查单的状态
        /// </summary>
        public WareHouseFormStatus Status
        {
            get { return status; }
            set { status = value; }
        }
        /// <summary>
        /// 检查时间
        /// </summary>
        public DateTime CheckDateFrom
        {
            get { return checkDateFrom; }
            set { checkDateFrom = value; }
        }
        /// <summary>
        /// 检查时间
        /// </summary>
        public DateTime CheckDateTo
        {
            get { return checkDateTo; }
            set { checkDateTo = value; }
        }
    }
}
